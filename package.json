    "@nestjs/cqrs": "^8.0.3",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.4.1",
    "@nestjs/typeorm": "^8.1.2",
    "@nestjs/websockets": "^8.4.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/cookie": "^0.4.1",
