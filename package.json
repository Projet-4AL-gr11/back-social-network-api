    "@types/passport-local": "^1.0.34",
    "@types/socket.io": "^3.0.2",
    "aws-sdk": "^2.1098.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie": "^0.4.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "node-mocks-http": "^1.11.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.45",
    "winston": "^3.3.3"
